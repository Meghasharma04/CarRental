<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>CarRental</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.ul.project.login.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewServlet</servlet-name>
    <servlet-class>com.ul.project.view.ViewServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewServlet</servlet-name>
    <url-pattern>/view</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddCarServlet</servlet-name>
    <servlet-class>com.ul.project.add.AddCarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddCarServlet</servlet-name>
    <url-pattern>/addCar</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ShowCarServlet</servlet-name>
    <servlet-class>com.ul.project.view.ShowCarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowCarServlet</servlet-name>
    <url-pattern>/ShowCarServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BookCarServlet</servlet-name>
    <servlet-class>com.ul.project.add.BookCarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookCarServlet</servlet-name>
    <url-pattern>/book</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.ul.project.add.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>
</web-app>